{
  "name": "extra-utils",
  "version": "4.0.1",
  "description": "Utilities for JavaScript and Typescript",
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-utils.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "ts-patch install -s",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.cjs",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --config jest.config.cjs",
    "test:coverage": "jest --coverage --config jest.config.cjs",
    "prepublishOnly": "run-s prepare clean build",
    "clean": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --outDir lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prepare lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@blackglory/pass": "^1.1.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "jest-resolve": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-patch": "^2.1.0",
    "tsd": "^0.25.0",
    "tslib": "^2.4.1",
    "typescript": "4.9.3",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "justypes": "^4.0.0",
    "lodash": "^4.17.21"
  }
}
