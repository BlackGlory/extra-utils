{
  "name": "extra-utils",
  "version": "1.4.0",
  "description": "Utilities for JavaScript and Typescript",
  "files": ["lib"],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "repository": "git@github.com:BlackGlory/extra-utils.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --runInBand --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@blackglory/pass": "^1.1.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^27.4.1",
    "@types/jsdom": "^16.2.15",
    "@types/lodash": "^4.14.186",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^27.1.4",
    "tscpaths": "^0.0.9",
    "tsd": "^0.20.0",
    "tslib": "^2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "justypes": "^3.0.0",
    "lodash": "^4.17.21"
  }
}
